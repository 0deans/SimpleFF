{
	"productName": "simpleff",
	"version": "0.1.0",
	"identifier": "me.0dean.simpleff",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build"
	},
	"app": {
		"windows": [
			{
				"title": "simpleff",
				"width": 360,
				"height": 420,
				"resizable": false,
				"fullscreen": false,
				"decorations": false,
				"shadow": false,
				"transparent": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
